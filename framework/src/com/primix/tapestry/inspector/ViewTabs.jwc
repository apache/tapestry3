<specification>
	<class>com.primix.tapestry.inspector.ViewTabs</class>
	<components>
		<component>
			<id>e</id>
			<type>Foreach</type>
			<bindings>
				<binding>
					<name>source</name>
					<property-path>views</property-path>
				</binding>
				<binding>
					<name>value</name>
					<property-path>view</property-path>
				</binding>
			</bindings>
		</component>
		<component>
			<id>select</id>
			<type>Action</type>
			<bindings>
				<binding>
					<name>listener</name>
					<property-path>selectListener</property-path>
				</binding>
			</bindings>
		</component>
		<component>
			<id>tab</id>
			<type>Rollover</type>
			<bindings>
				<binding>
					<name>image</name>
					<property-path>viewImage</property-path>
				</binding>
				<binding>
					<name>focus</name>
					<property-path>focusImage</property-path>
				</binding>
			</bindings>
		</component>
		<component>
			<id>banner</id>
			<type>Image</type>
			<bindings>
				<binding>
					<name>image</name>
					<property-path>bannerImage</property-path>
				</binding>
			</bindings>
		</component>
		<component>
			<id>wrapped</id>
			<type>InsertWrapped</type>
		</component>
	</components>
	<assets>

<!DOCTYPE specification PUBLIC "-//Howard Ship//Tapestry Specification 1.1//EN" "http://tapestry.sf.net/dtd/Tapestry_1_1.dtd">
<specification class="com.primix.tapestry.valid.DateField">
  <parameter name="date" java-type="java.util.Date" required="yes"/>
  <parameter name="delegate" java-type="com.primix.tapestry.valid.IValidationDelegate" required="yes"/>
  <parameter name="displayName" java-type="java.lang.String" required="yes"/>
  <parameter name="maximum" java-type="java.util.Date"/>
  <parameter name="minimum" java-type="java.util.Date"/>
  <parameter name="required"/>
  <parameter name="format" java-type="java.text.DateFormat"/>
  <component id="captive" type="TextField">
    <binding name="text" property-path="text"/>
    <static-binding name="displayWidth">10</static-binding>
    <static-binding name="maximumLength">10</static-binding>
  </component>
</specification>

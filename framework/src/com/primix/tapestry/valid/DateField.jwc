<specification>
	<class>com.primix.tapestry.components.html.valid.DateField</class>
	<parameters>
		<allow-informal-parameters>no</allow-informal-parameters>
		<parameter>
			<name>date</name>
			<java-type>java.util.Date</java-type>
			<required>yes</required>
		</parameter>
		<parameter>
			<name>delegate</name>
			<java-type>com.primix.tapestry.components.html.valid.IValidationDelegate</java-type>
			<required>yes</required>
		</parameter>
		<parameter>
			<name>disabled</name>
		</parameter>
		<parameter>
			<name>displayName</name>
			<java-type>java.lang.String</java-type>
			<required>yes</required>
		</parameter>
		<parameter>
			<name>maximum</name>
			<java-type>java.util.Date</java-type>
		</parameter>
		<parameter>
			<name>minimum</name>
			<java-type>java.util.Date</java-type>
		</parameter>
		<parameter>
			<name>required</name>
		</parameter>
	</parameters>
	<components>
		<component>
			<id>captive</id>
			<type>TextField</type>
			<bindings>
				<binding>
					<name>text</name>
					<property-path>text</property-path>
				</binding>
				<static-binding>
					<name>displayWidth</name>
					<value>10</value>
				</static-binding>
				<static-binding>
					<name>maximumLength</name>
					<value>10</value>
				</static-binding>
				<inherited-binding>
					<name>disabled</name>
					<parameter-name>disabled</parameter-name>
				</inherited-binding>
			</bindings>
		</component>
	</components>

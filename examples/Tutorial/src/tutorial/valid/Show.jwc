<?xml version="1.0" encoding="UTF-8"?>
<!-- $Id$ -->
<!DOCTYPE specification PUBLIC "-//Howard Ship//Tapestry Specification 1.1//EN"
	"http://tapestry.sf.net/dtd/Tapestry_1_1.dtd">
<specification class="com.primix.tapestry.BasePage">
  <component id="border" type="Border"/>
  <component id="insertStringData" type="Insert">
    <binding name="value" property-path="visit.stringData"/>
  </component>
  <component id="insertIntData" type="Insert">
    <binding name="value" property-path="visit.intData"/>
  </component>
  <component id="insertDoubleData" type="Insert">
    <binding name="value" property-path="visit.doubleData"/>
  </component>
  <component id="insertDecimalData" type="Insert">
    <binding name="value" property-path="visit.decimalData"/>
  </component>
  <component id="insertBigIntData" type="Insert">
    <binding name="value" property-path="visit.bigIntData"/>
  </component>
  <component id="home" type="Page">
    <static-binding name="page">Home</static-binding>
  </component>
</specification>

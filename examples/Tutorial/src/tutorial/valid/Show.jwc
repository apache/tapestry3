<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE specification PUBLIC "-//Primix Solutions//Tapestry Specification 1.0//EN"
	"http://tapestry.sourceforge.net/dtd/Tapestry_1_0.dtd">
<specification>
  <class>com.primix.tapestry.BasePage</class>
  <components>
    <component>
      <id>border</id>
      <type>Border</type>
    </component>
    <component>
      <id>insertStringData</id>
      <type>Insert</type>
      <bindings>
        <binding>
          <name>value</name>
          <property-path>visit.stringData</property-path>
        </binding>
      </bindings>
    </component>
    <component>
      <id>insertIntData</id>
      <type>Insert</type>
      <bindings>
        <binding>
          <name>value</name>
          <property-path>visit.intData</property-path>
        </binding>
      </bindings>
    </component>
    <component>
      <id>insertDoubleData</id>
      <type>Insert</type>
      <bindings>
        <binding>
          <name>value</name>
          <property-path>visit.doubleData</property-path>
        </binding>
      </bindings>
    </component>
    <component>
      <id>insertDecimalData</id>
      <type>Insert</type>
      <bindings>
        <binding>
          <name>value</name>
          <property-path>visit.decimalData</property-path>
        </binding>
      </bindings>
    </component>
    <component>
      <id>insertBigIntData</id>
      <type>Insert</type>
      <bindings>
        <binding>
          <name>value</name>
          <property-path>visit.bigIntData</property-path>
        </binding>
      </bindings>
    </component>
    <component>
      <id>home</id>
      <type>Page</type>
      <bindings>
        <static-binding>
          <name>page</name>
          <value>Home</value>
        </static-binding>
      </bindings>
    </component>
  </components>
</specification>
